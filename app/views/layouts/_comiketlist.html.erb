<% list = locals[:contents] %>
<h4><%= list.size %>件の購買リストがアップロードされています</h4>
<table class="table table-striped table-bordered table-list">
  <thead>
  <tr>
    <th>日付</th>
    <th>地区</th>
    <th>スペース</th>
    <th>島</th>
    <th>サークル名</th>
    <th>属性</th>
    <th>アイテム</th>
    <th>単価</th>
    <th>発注数</th>
    <th>発注者</th>
    <th>備考</th>
    <th>ポイント</th>
  </tr>
  </thead>
  <tbody>
  <% list.each do |ent| %>
    <tr>
      <td><%= ent.date %></td>
      <td><%= ent.chiku %></td>
      <td><%= ent.space %></td>
      <td><%= ent.shima %></td>
      <td><%= ent.circle_name %></td>
      <td><%= ent.zokusei %></td>
      <td><%= ent.item %></td>
      <td><%= ent.tanka %></td>
      <td><%= ent.hattyusu %></td>
      <td><%= ent.hattyusha %></td>
      <td><%= ent.bikou %></td>
      <td><%= ent.point %></td>
    </tr>
  <% end %>
  </tbody>
</table>
